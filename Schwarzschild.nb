 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["e", "TI"], 
         SubsuperscriptBox["\[Null]", "\[Nu]", 
          StyleBox["m", "TI"]], 
         StyleBox["e", "TI"], 
         SubsuperscriptBox["\[Null]", 
          StyleBox["m", "TI"], "\[Mu]"], "\[LongEqual]", 
         SubsuperscriptBox["\[Delta]", "\[Nu]", "\[Mu]"]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "$ \\T{e}{^m_\\nu} \\T{e}{_m^\\mu} = \\delta^\\mu_\\nu $", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "d462adeb-6caf-4393-8865-7fde0695b34e"],
 " and ",
