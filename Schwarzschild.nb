Cell[TextData[Cell[BoxData[
 FormBox[
  TemplateBox[<|"boxes" -> FormBox[
      RowBox[{
        SubscriptBox["\[Omega]", "\[Mu]"], "\[LongEqual]", "\[Omega]", 
        RowBox[{"(", 
          SubscriptBox[
           StyleBox["g", "TI"], "\[Mu]"], ")"}], "\[LongEqual]", 
        FractionBox["1", "2"], 
        RowBox[{"[", 
          RowBox[{
            RowBox[{"(", 
              RowBox[{
                SubscriptBox["\[Gamma]", "\[Alpha]"], "\[Wedge]", 
                StyleBox["D", "TI"], "\[Wedge]", 
                SubscriptBox["\[Gamma]", "\[Alpha]"]}], ")"}], "\[CenterDot]", 
            SubscriptBox[
             StyleBox["g", "TI"], "\[Mu]"]}], "]"}], "-", 
        SubsuperscriptBox[
         StyleBox["h", "TI"], "\[Mu]", "\[Nu]"], 
        StyleBox["D", "TI"], "\[Wedge]", 
        SubscriptBox["\[Gamma]", "\[Nu]"]}], TraditionalForm], "errors" -> {},
     "input" -> 
    "\\omega_{\\mu}=\\omega ( g_{\\mu})=\\frac{1}{2}\\left[ (\\gamma_\\alpha \
\\wedge D \\wedge \\gamma_\\alpha) \\cdot g_\\mu\\right] - h^\\nu_\\mu D \
\\wedge \\gamma_\\nu", "state" -> "Boxes"|>,
   "TeXAssistantTemplate"], 
  TraditionalForm]],ExpressionUUID->"7408fd8d-c92a-4ac6-a820-7f7260d0d0fa"]], \
"Text",
 CellChangeTimes->{3.907918571167731*^9, 
  3.908254451869863*^9},ExpressionUUID->"3c5a0c95-0250-432d-b8cc-\
ba029e11adcc"],

Cell[TextData[{
 "The result is given in the form ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox["\[Omega]", "\[Mu]"], "\[LongEqual]", 
         UnderscriptBox["\[Sum]", 
          RowBox[{
            StyleBox["l", "TI"], ",", 
            StyleBox["m", "TI"]}], LimitsPositioning -> True], 
         SubscriptBox["\[Omega]", 
          RowBox[{
            StyleBox["l", "TI"], 
            StyleBox["m", "TI"], "\[Mu]"}]], 
         SubscriptBox["\[Gamma]", 
          StyleBox["l", "TI"]], "\[Wedge]", 
         SubscriptBox["\[Gamma]", 
          StyleBox["m", "TI"]]}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\omega_\\mu = \\sum_{l,m} \\omega_{l m \\mu} \\gamma_l \\wedge \
\\gamma_m", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "931f385e-1830-4e03-9c42-2951549c886b"]
}], "Text",
 CellChangeTimes->{{3.90791860717034*^9, 3.907918613913926*^9}, {
  3.936067899171084*^9, 
  3.936067899432646*^9}},ExpressionUUID->"c5942b9a-4d53-47a9-9251-\
90986b343380"],

Cell[TextData[{
 "We start by creating the function ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["D", "TI"], "\[Wedge]", 
         SubscriptBox["\[Gamma]", 
          StyleBox["l", "TI"]], "\[LongEqual]", 
         UnderoverscriptBox["\[Sum]", 
          RowBox[{
            StyleBox["m", "TI"], ",", "\[Mu]", ",", "\[Nu]", ",", 
            StyleBox["n", "TI"], "\[LongEqual]", "1"}], "4", 
          LimitsPositioning -> True], 
         SubscriptBox["\[Eta]", 
          RowBox[{
            StyleBox["l", "TI"], 
            StyleBox["l", "TI"]}]], 
         SuperscriptBox["\[Eta]", 
          RowBox[{
            StyleBox["m", "TI"], 
            StyleBox["m", "TI"]}]], 
         SuperscriptBox["\[Eta]", 
          RowBox[{
            StyleBox["n", "TI"], 
            StyleBox["n", "TI"]}]], 
         SubsuperscriptBox[
          StyleBox["e", "TI"], 
          StyleBox["m", "TI"], 
          RowBox[{".", "\[Mu]"}]], 
         SubsuperscriptBox[
          StyleBox["e", "TI"], 
          StyleBox["n", "TI"], 
          RowBox[{".", "\[Nu]"}]], 
         RowBox[{"(", 
           RowBox[{
             SubscriptBox["\[PartialD]", "\[Mu]"], 
             SubsuperscriptBox[
              StyleBox["e", "TI"], 
              RowBox[{".", "\[Nu]"}], 
              StyleBox["l", "TI"]]}], ")"}], 
         SubscriptBox["\[Gamma]", 
          StyleBox["m", "TI"]], "\[Wedge]", 
         SubscriptBox["\[Gamma]", 
          StyleBox["n", "TI"]]}], TraditionalForm], "errors" -> {}, "input" -> 
     "D \\wedge \\gamma_l = \
\\sum_{m,\\mu,\\nu,n=1}^4\\eta_{ll}\\eta^{mm}\\eta^{nn}e_m^{.\\mu}e_n^{.\\nu}(\
\\partial_\\mu e^l_{.\\nu})\\gamma_m\\wedge \\gamma_n", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "268e861e-92b0-4bd7-a036-f6a2a33526b3"],
 "\nWhere we used the fact that the Minkowski metric is diagonal."
}], "Text",
 CellChangeTimes->{
